{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
      "name",
      "password"
  ],
  "properties": {
      "name": {
          "type": "string",
          "minLength": 1,
          "maxLength": 30,
          "title": "The name Schema",
          "examples": [
              "testsession"
          ]
      },
      "password": {
          "type": "string",
          "minLength": 5,
          "maxLength": 20,
          "title": "The password Schema",
          "examples": [
              "password"
          ]
      },
      "description": {
          "type": "string",
          "minLength": 1,
          "title": "The description Schema",
          "examples": [
              "testdescription"
          ]
      },
      "campaignId": {
          "type": "integer",
          "title": "The campaignId Schema",
          "examples": [
              2
          ]
      },
      "dungeonMasterId": {
          "type": "integer",
          "title": "The dungeonMasterId Schema",
          "examples": [
            2
          ]
      }
  },
  "examples": [{
      "name": "testname",
      "password": "password",
      "description": "testdescription",
      "campaignId": 2,
      "dungeonMasterId": 2
  }]
}